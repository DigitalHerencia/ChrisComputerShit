{
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "singleQuote": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "semi": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "importOrder": [
    "^next",
    "^@clerk/(.*)",
    "^@neondatabase/(.*)",
    "^@prisma/(.*)",
    "^@radix-ui/(.*)",
    "^@sonner/(.*)",
    "^@supabase/(.*)",
    "^@vaul/(.*)",
    "^react",
    "^react-(.*)",
    "^typescript",
    "^zod",
    "^clsx",
    "^[./]"
  ],
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "printWidth": 80,
        "proseWrap": "always"
      }
    },
    {
      "files": "*.json",
      "options": {
        "printWidth": 80,
        "bracketSpacing": false,
        "singleQuote": false
      }
    }
  ]
}
